{
    "name": "obsidian-moon/framework",
    "description": "An Open Source, Lightweight and 100% Modular Framework in PHP",
    "version": "2.1.0",
    "type": "project",
    "keywords": [
        "framework"
    ],
    "homepage": "https://github.com/obsidian-moon/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Alfonso Martinez",
            "email": "opensaurusrex@gmail.com",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "obsidian-moon/engine": "^2.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "classmap": ["app/"],
        "files": ["app/mix.php"],
        "psr-4": {
            "ObsidianMoon\\Framework\\": "app/"
        }
    },
    "config": {
        "preferred-install": {
            "roave/security-advisories": "source",
            "*": "dist"
        }
    },
    "minimum-stability": "stable"
}
